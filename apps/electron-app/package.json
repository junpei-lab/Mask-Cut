{
  "name": "@mask-cut/electron-app",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/main/main.js",
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "build": "pnpm run clean && pnpm exec tsc -p tsconfig.json && pnpm exec tsc -p tsconfig.renderer.json && node ./scripts/fixRendererImports.mjs && pnpm exec cpx \"src/renderer/**/*.html\" dist/renderer && pnpm exec cpx \"../../assets/**/*\" dist/assets",
    "start": "pnpm run build && electron .",
    "dev": "pnpm run build && electron .",
    "lint": "tsc --noEmit",
    "test": "node --require ts-node/register --test src/**/*.test.ts"
  },
  "dependencies": {
    "@mask-cut/text-llm-core": "workspace:*"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "electron": "^28.2.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
